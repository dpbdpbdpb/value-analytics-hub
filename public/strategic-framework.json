    {
      "id": "A1-orthopedic-consolidation",
      "name": "Orthopedic Consolidation (A1)",
      "programEpicId": "A1",
      "initiativeId": "EPIC-001",
      "categoryId": "vendor-selection",
      "description": "73% vendor reduction (38‚Üí10), $15M savings, methodology validation",
